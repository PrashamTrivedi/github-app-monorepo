{
  "name": "@github-app/ui",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub App frontend UI with Next.js and Cloudflare Pages",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist",
    "deploy": "wrangler pages deploy .next --project-name github-app-ui",
    "preview": "wrangler pages dev .next --port 3000",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@github-app/shared": "workspace:*",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@tailwindcss/forms": "^0.5.7",
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "wrangler": "^3.25.0"
  }
}