{
  "name": "github-app-ui",
  "compatibility_date": "2024-01-15",
  "pages_build_output_dir": ".next",
  "env": {
    "production": {
      "vars": {
        "NEXT_PUBLIC_API_BASE_URL": "https://github-app-backend.your-subdomain.workers.dev",
        "ENVIRONMENT": "production",
        "NEXT_PUBLIC_GITHUB_APP_NAME": "your-github-app-name"
      }
    },
    "staging": {
      "vars": {
        "NEXT_PUBLIC_API_BASE_URL": "https://github-app-backend-staging.your-subdomain.workers.dev",
        "ENVIRONMENT": "staging",
        "NEXT_PUBLIC_GITHUB_APP_NAME": "your-github-app-name"
      }
    }
  },
  "vars": {
    "NEXT_PUBLIC_API_BASE_URL": "http://localhost:8787",
    "ENVIRONMENT": "development",
    "NEXT_PUBLIC_GITHUB_APP_NAME": "your-github-app-name"
  }
}